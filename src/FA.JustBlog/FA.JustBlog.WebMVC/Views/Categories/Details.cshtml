@model IEnumerable<FA.JustBlog.Models.Common.Post>
@{
    ViewBag.Title = ViewBag.CategoryName;
}
<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.6.3/css/all.css" integrity="sha384-UHRtZLI+pbxtHCWp1t77Bi1L4ZtiqrqD80Kn4Z8NTSRyMA2Fd33n5dQ8lWUE00s/" crossorigin="anonymous">
@foreach (var item in Model)
{
    <img src="~/asset/images/@item.ImageUrl" class="card-img-top" alt="@item.ImageUrl">
    <h1>@item.Title</h1>
    <div><b>Published Date: </b> @item.PublishedDate</div>
    <div><b>Url Slug: </b> @item.UrlSlug</div>
    <div><b>Content: </b> @item.PostContent</div>
}
